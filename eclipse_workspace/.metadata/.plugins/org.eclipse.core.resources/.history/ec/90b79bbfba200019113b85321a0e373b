#include "opencv2/opencv.hpp"
#include "dm04_mat.h"
#include <iostream>
#include <cmath>

using namespace cv;
using namespace std;

int test_dm04() {
	Mat src = imread("/Users/lisujia/Desktop/test.jpg");
	if (src.empty()) {
		return -1;


	}

	namedWindow("input", WINDOW_AUTOSIZE);
	imshow("intput", src);

//	Mat dst = Mat(src.size(), src.type());
	Mat dst = src.clone();


	namedWindow("output", WINDOW_AUTOSIZE);
	imshow("output", dst);
	waitKey(0);
	return 0;
}

